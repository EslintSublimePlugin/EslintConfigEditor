        "valid-typeof": "error",
        "angular/dumb-inject": 0,
        "angular/component-name": 0,
        "key-spacing": "off",
        "no-inline-comments": "off",
        "semi": "off",
        "no-empty-character-class": "error",
        "init-declarations": "off",
        "no-empty": "error",
        "no-proto": "off",
        "id-blacklist": "off",
        "object-curly-spacing": "off",
        "no-continue": "off",
